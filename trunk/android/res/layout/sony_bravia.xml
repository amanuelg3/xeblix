<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout android:id="@+id/AbsoluteLayout01"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyPower"
		android:text="Power" android:layout_y="10dip" android:layout_x="240dip" android:minWidth="75px" android:minHeight="60px"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyMenu"
		android:text="Menu" android:layout_y="10dip" android:layout_x="20dip" android:minWidth="75px" android:minHeight="60px"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyUp"
		android:text="  Up  " android:layout_x="130dip" android:minWidth="75px" android:minHeight="60px" android:layout_y="50dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyEnter"
		android:text="Enter" android:layout_x="130dip" android:layout_y="110dip" android:phoneNumber="false" android:minWidth="75px" android:minHeight="60px"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyLeft"
		android:layout_y="110dip" android:text=" Left" android:minWidth="75px" android:minHeight="60px" android:layout_x="55dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyRight"
		android:text="Right" android:layout_y="110dip" android:minWidth="75px" android:minHeight="60px" android:layout_x="205dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyDown"
		android:text="Down" android:minWidth="75px" android:minHeight="60px" android:layout_x="130dip" android:layout_y="170dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyVolumeUp"
		android:text="Volume +" android:layout_x="20dip" android:layout_y="210dip" android:minWidth="75px" android:minHeight="60px"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyVolumeDown"
		android:layout_x="20dip" android:text="Volume - " android:minWidth="75px" android:minHeight="60px" android:layout_y="270dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyChannelUp"
		android:text="Channel +" android:layout_y="210dip" android:layout_x="220dip" android:minWidth="75px" android:minHeight="60px"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyChannelDown"
		android:text="Channel - " android:layout_x="220dip" android:minWidth="75px" android:minHeight="60px" android:layout_y="270dip"></Button>
	<Button android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/sonyMute"
		android:text="Mute" android:layout_x="130dip" android:layout_y="230dip" android:minWidth="75px" android:minHeight="60px"></Button>
</AbsoluteLayout>
